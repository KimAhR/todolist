ntext);\r\n\r\n  // context가 null일 경우 : Provider 밖이다.\r\n  if (!context) throw Error(\"Provider 없음\");\r\n\r\n  return context;\r\n}\r\n\r\nexport function useTodoDispatch() {\r\n  const context = useContext(TodoDispatchContext);\r\n\r\n  if (!context) throw Error(\"Provider 없음\");\r\n\r\n  return context;\r\n}\r\n","C:\\Users\\Neppplus\\Desktop\\포폴\\todolist\\src\\index.js",[],[],"C:\\Users\\Neppplus\\Desktop\\포폴\\todolist\\src\\App.js",[],[],"C:\\Users\\Neppplus\\Desktop\\포폴\\todolist\\src\\style.js",[],[],"C:\\Users\\Neppplus\\Desktop\\포폴\\todolist\\src\\component\\todos\\Todos.jsx",["116","117","118","119"],[],"C:\\Users\\Neppplus\\Desktop\\포폴\\todolist\\src\\component\\todos\\TodoBody.jsx",[],[],"C:\\Users\\Neppplus\\Desktop\\포폴\\todolist\\src\\component\\todos\\TodoHeader.jsx",[],[],"C:\\Users\\Neppplus\\Desktop\\포폴\\todolist\\src\\context\\todos.js",["120"],[],"C:\\Users\\Neppplus\\Desktop\\포폴\\todolist\\src\\component\\todos\\TodoInput.jsx",[],[],"C:\\Users\\Neppplus\\Desktop\\포폴\\todolist\\src\\component\\todos\\TodoItem.jsx",[],[],{"ruleId":"121","severity":1,"message":"122","line":1,"column":10,"nodeType":"123","messageId":"124","endLine":1,"endColumn":20},{"ruleId":"121","severity":1,"message":"125","line":1,"column":22,"nodeType":"123","messageId":"124","endLine":1,"endColumn":32},{"ruleId":"121","severity":1,"message":"126","line":3,"column":10,"nodeType":"123","messageId":"124","endLine":3,"endColumn":20},{"ruleId":"121","severity":1,"message":"127","line":3,"column":36,"nodeType":"123","messageId":"124","endLine":3,"endColumn":48},{"ruleId":"121","severity":1,"message":"128","line":2,"column":49,"nodeType":"123","messageId":"124","endLine":2,"endColumn":57},{"ruleId":"121","severity":1,"message":"122","line":1,"column":10,"nodeType":"123","messageId":"124","endLine":1,"endColumn":20},{"ruleId":"121","severity":1,"message":"125","line":1,"column":22,"nodeType":"123","messageId":"124","endLine":1,"endColumn":32},{"ruleId":"121","severity":1,"message":"